{% extends 'base.html.twig' %}

{% block title %}{{ order.productId }}{% endblock %}

{% block body %}
    <h1>Order</h1>

    <table class="table">
        <tbody>
        <tr>
            <th>Product</th>
            <td>{{ order.productId }}</td>
        </tr>
        <tr>
            <th>Count</th>
            <td>{{ order.count }}</td>
        </tr>
    </table>

{% endblock %}

